{
  "name": "kompo",
  "version": "0.2.1",
  "description": "Kompo is a react like library which tries to stay simple as possible. No virtual DOM or JSX. Kompo loves the DOM.",
  "main": "./dist/kompo.js",
  "scripts": {
    "commit": "git-cz",
    "test": "./node_modules/.bin/karma start --single-run",
    "testAll": "./node_modules/.bin/karma start",
    "watch": "./node_modules/.bin/webpack -w",
    "build": "./node_modules/.bin/webpack",
    "lint": "./node_modules/.bin/eslint src/**"
  },
  "keywords": [
    "react",
    "view",
    "components",
    "javascript",
    "interface",
    "DOM"
  ],
  "author": "Ruben Hazelaar <ruben.hazelaar@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "6.4.0",
    "babel-loader": "6.2.1",
    "babel-plugin-transform-es2015-classes": "6.4.5",
    "babel-plugin-transform-object-assign": "6.3.13",
    "babel-preset-es2015": "6.3.13",
    "babel-register": "6.3.13",
    "chai": "3.4.1",
    "clean-webpack-plugin": "0.1.6",
    "commitizen": "2.5.0",
    "cz-conventional-changelog": "1.1.5",
    "eslint": "1.10.3",
    "eslint-loader": "1.2.0",
    "karma": "0.13.19",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "0.2.2",
    "karma-firefox-launcher": "0.1.7",
    "karma-ie-launcher": "0.2.0",
    "karma-mocha": "0.2.1",
    "karma-safari-launcher": "0.1.1",
    "karma-sinon": "1.0.4",
    "karma-sourcemap-loader": "0.3.6",
    "karma-webpack": "1.7.0",
    "mocha": "2.3.4",
    "sinon": "1.17.2",
    "webpack": "1.12.10"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
